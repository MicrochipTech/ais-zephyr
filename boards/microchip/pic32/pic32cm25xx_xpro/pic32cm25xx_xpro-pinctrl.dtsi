#include <dt-bindings/pinctrl/pic32-cm25xx-pinctrl.h>

&pinctrl {
	sercom0_uart_default: sercom0_uart_default {
		group1 {
			pinmux = <DIO0_SERCOM0_PAD>,
				 <DIO1_SERCOM0_PAD>;
		};
	};

	sercom1_uart_default: sercom1_uart_default {
		group1 {
			pinmux = <DIO4_SERCOM1_PAD>,
				 <DIO5_SERCOM1_PAD>;
		};
	};

	sercom2_uart_default: sercom2_uart_default {
		group1 {
			pinmux = <DIO8_SERCOM2_PAD>,
				 <DIO9_SERCOM2_PAD>;
		};
	};

	sercom3_uart_default: sercom3_uart_default {
		group1 {
			pinmux = <DIO12_SERCOM3_PAD>,
				 <DIO13_SERCOM3_PAD>;
		};
	};
};
