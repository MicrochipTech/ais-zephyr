config SOC_FAMILY_MICROCHIP_PIC32
	bool

config SOC_FAMILY
	default "microchip_pic32" if SOC_FAMILY_MICROCHIP_PIC32

config SOC_SERIES_REVISION_N
	bool
	depends on SOC_FAMILY_MICROCHIP_PIC32

config SOC_SERIES_REVISION
	string
	default "n" if SOC_SERIES_REVISION_N
	default ""
	depends on SOC_FAMILY_MICROCHIP_PIC32

rsource "*/Kconfig.soc"
